            <fontsize size="25"/>
            <!--<fontstyle style="3"/>-->
            <text str="Mulit-line input" x="0" y="0"/>
            <text str="Text Area" x="0" y="26"/>
        </foreground>
        <background>
            <strokecolor color="grey"/>
            <fillcolor color="white"/>
            <rect x="0" y="0" w="171" h="70"/>   
            <fillstroke/>
