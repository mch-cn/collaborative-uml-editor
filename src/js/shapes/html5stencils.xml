            <text str="Button" x="85" y="10" align="center" valign="middle"/>
        </foreground>
        <background>
            <strokecolor color="#a4a4a4"/>
            <fillcolor color="#e3e3e4"/>
            <roundrect x="0" y="0" w="171" h="25" arcsize="10.851851851851851"/>   
            <fillstroke/>
        </background>
</shape>
<shape name="textnode" h="25" w="171" aspect="1">
        <foreground>
            <fontsize size="50"/>
            <fontstyle style="1"/>
            <text str="Text" x="85" y="10" align="center" valign="middle"/>
        </foreground>
</shape>
